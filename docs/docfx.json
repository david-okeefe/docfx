{
  "build": {
    "content": [
      {
        "files": [
          "policies/**/*.{md,yml}",
          "processes/**/*.{md,yml}",
          "procedures/**/*.{md,yml}",
          "guides/**/*.{md,yml}",
          "best-practices/**/*.{md,yml}"
        ],
        "exclude": [
          "_site/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          ".attachments/**",
          "images/**"
        ]
      }
    ],
    "output": "_site",
    "template": [
      "default",
      "modern"
    ],
    "globalMetadata": {
      "_appName": "docs",
      "_appLogoPath": "images/americas-gps-logo.png",
      "_appFaviconPath": "images/americas-gps-favicon.ico",
      "_enableSearch": true,
      "_enableNewTab": true,
      "_disableNextArticle": true,
      "_gitContribute": {
          "repo": "https://github.com/david-okeefe/docfx",
          "branch": "main"
      }
    },
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "postProcessors": [],
    "noLangKeyword": false,
    "keepFileLink": false,
    "disableGitFeatures": false,
    "markdownEngineName": "markdig",
    "markdownEngineProperties": {
      "markdigExtensions": [
        "tasklists",
        "diagrams"
      ]
    }
  }
}
